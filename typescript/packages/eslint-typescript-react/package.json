{
  "name": "eslint-config-entva-typescript",
  "description": "Shareable typescript styleguide config for eslint",
  "author": "Max Degterev <max@degterev.me>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/entva/styleguide"
  },
  "bugs": "https://github.com/entva/styleguide/issues",
  "version": "2.0.0",
  "keywords": [
    "linter",
    "config",
    "eslint",
    "typescript"
  ],
  "type": "module",
  "main": "index.js",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "test": "node ../../../test ./test js ts",
    "rules": "node_modules/.bin/eslint --print-config file.js > eslint-ruleset-output.json",
    "prepublishOnly": "npm test"
  },
  "devDependencies": {
    "react": "^19.0.0",
    "eslint": "^9.16.0"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^8.18.0",
    "eslint-config-entva": "^2.0.0",
    "eslint-config-entva-typescript-base": "^2.0.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^15.13.0"
  },
  "peerDependencies": {
    "eslint": "^9.x.x"
  }
}
